pipeline {
    agent any
    

stages{
        stage('Build'){
            steps {
                echo 'mvn clean package'
            }
            
        }

        stage ('Deployments'){
            
                    steps {
                        echo "scp -i /home/jenkins/tomcat-demo.pem **/target/*.war ec2-user@${params.tomcat_dev}:/var/lib/tomcat7/webapps"
                    }


            }
        }
    
}